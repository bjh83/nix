ENTRY(_start)

MEMORY {
  ram : ORIGIN = 0x80300000, LENGTH = 0x10000
}

SECTIONS {
  .text : {
    boot/entry.o(.text)
    *(.text)
    *(.rodata)
    _etext = .;
  } > ram

  .data : {
    _data = .;
    *(.data)
    _edata = .;
  } > ram

  .bss : {
    __bss_start = .;
    *(.bss)
  } > ram
  . = ALIGN(4);
  __bss_end = .;
  __stack_bottom = . + 0x5000;
}
